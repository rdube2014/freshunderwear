<h1>Editing User #<%= @user.id %></h1>

<%= form_tag(user_url(@user), method: 'patch') do %>
  <div>
    <%= label_tag :user_number %><br />
    <%= number_field_tag :user_number, @user.user_number %>
  </div>

  <div>
    <%= label_tag :first_name %><br />
    <%= text_field_tag :first_name, @user.first_name %>
  </div>

  <div>
    <%= label_tag :last_name %><br />
    <%= text_field_tag :last_name, @user.last_name %>
  </div>

  <div>
    <%= label_tag :home_city %><br />
    <%= text_field_tag :home_city, @user.home_city %>
  </div>

  <div>
    <%= label_tag :home_state %><br />
    <%= text_field_tag :home_state, @user.home_state %>
  </div>

  <div>
    <%= label_tag :home_country %><br />
    <%= text_field_tag :home_country, @user.home_country %>
  </div>

  <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email, @user.email %>
  </div>

  <div>
    <%= label_tag :user_name %><br />
    <%= text_field_tag :user_name, @user.user_name %>
  </div>

  <div>
    <%= label_tag :password %><br />
    <%= text_field_tag :password, @user.password %>
  </div>

  <div>
    <%= label_tag :security_question %><br />
    <%= text_field_tag :security_question, @user.security_question %>
  </div>

  <div>
    <%= label_tag :security_answer %><br />
    <%= text_field_tag :security_answer, @user.security_answer %>
  </div>

  <div>
    <%= label_tag :photo_link %><br />
    <%= text_field_tag :photo_link, @user.photo_link %>
  </div>

  <div>
    <%= submit_tag "Update User" %>
  </div>
<% end %>
